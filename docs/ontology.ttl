 ] .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean
 ] .

#################################################################
#    Annotations
#################################################################

:Agent rdfs:comment "An autonomous entity capable of having roles, goals, and capabilities within a hybrid intelligence system."@en ;
        rdfs:label "Agent"@en .


#################################################################
#    Rules
#################################################################

<https://w3id.org/hi-ontology/rules/team> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<https://w3id.org/hi-ontology/rules/agent> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<https://w3id.org/hi-ontology/rules/role> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<https://w3id.org/hi-ontology/rules/goal> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<https://w3id.org/hi-ontology/rules/task> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<https://w3id.org/hi-ontology/rules/cap> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

