                                 <http://purl.org/vocab/vann/preferredNamespacePrefix> "hi" ;
                                 <http://purl.org/vocab/vann/preferredNamespaceUri> "https://w3id.org/hi-ontology#" ;
                                 owl:versionInfo "2.0.0" .

