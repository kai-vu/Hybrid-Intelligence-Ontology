    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy -->


    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy"/>
    


    <!-- https://w3id.org/hi-ontology#allowsTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#allowsTask">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Capability"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Task"/>
        <rdfs:comment xml:lang="en">A capability enables or allows a task to be performed.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#assignedToTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#assignedToTask">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Task"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#performsExecution"/>
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#realizesTask"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Agent formally assigned to perform a task (inferred from executions).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#describesUseCase -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#describesUseCase">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#ScientificPaper"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#UseCase"/>
        <rdfs:comment xml:lang="en">A paper describing a specific use case or scenario.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#eligibleForTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#eligibleForTask">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hi-ontology#assignedToTask"/>
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Task"/>
        <rdfs:comment xml:lang="en">Agent eligible for a task based on membership and capabilities (policy-level notion).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#evaluatedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#evaluatedBy">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#TaskExecution"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Evaluation"/>
        <rdfs:comment xml:lang="en">Execution is evaluated by a defined evaluation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#executesTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#executesTask">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hi-ontology#assignedToTask"/>
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Task"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#performsExecution"/>
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#realizesTask"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Agent executes a task (derivable from Agent→Execution→Task).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#exhibitsPhenomenon -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#exhibitsPhenomenon">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Context"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#AgentRelatedPhenomenon"/>
        <rdfs:comment xml:lang="en">Context exhibits a phenomenon (e.g., bias/feedback emerges in that context).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#hasAlternativeHypothesis -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#hasAlternativeHypothesis">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Evaluation"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#AlternativeHypothesis"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#hasCapability -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#hasCapability">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Capability"/>
        <rdfs:comment xml:lang="en">Agent possesses a capability.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#hasEthicalConstraint -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#hasEthicalConstraint">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Context"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#EthicalConstraint"/>
        <rdfs:comment xml:lang="en">Ethical constraints that apply in a given context.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#hasExperiment -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#hasExperiment">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Evaluation"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Experiment"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#hasGoal -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#hasGoal">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#HITeam"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Goal"/>
        <rdfs:comment xml:lang="en">An HI team pursues one or more goals.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#hasInteractionIntent -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#hasInteractionIntent">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Interaction"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Intentionality"/>
        <rdfs:comment xml:lang="en">Intent underlying the interaction (e.g., explain, request, negotiate).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#hasMember -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#hasMember">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#HITeam"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:comment xml:lang="en">Specifies agents that are members of an HI team.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#hasNullHypothesis -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#hasNullHypothesis">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Evaluation"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#NullHypothesis"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#hasRole -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#hasRole">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#inDomain -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#inDomain">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#UseCase"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Domain"/>
        <rdfs:comment xml:lang="en">Links a use case to its domain of application.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#influencedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#influencedBy">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/hi-ontology#AgentRelatedPhenomenon"/>
                    <rdf:Description rdf:about="https://w3id.org/hi-ontology#EthicalConstraint"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Indicates that an entity is influenced by a phenomenon or ethical constraint.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#influencedByEthicalConstraint -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#influencedByEthicalConstraint">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method"/>
                    <rdf:Description rdf:about="https://w3id.org/hi-ontology#Agent"/>
                    <rdf:Description rdf:about="https://w3id.org/hi-ontology#Evaluation"/>
                    <rdf:Description rdf:about="https://w3id.org/hi-ontology#TaskExecution"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#EthicalConstraint"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#operatesInContext"/>
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#hasEthicalConstraint"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Direct influence of an ethical constraint on agents, executions, methods, or evaluations.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#introducesHITeam -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#introducesHITeam">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#UseCase"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#HITeam"/>
        <rdfs:comment xml:lang="en">A use case introduces a hybrid intelligence team.</rdfs:comment>
    </owl:ObjectProperty>
    


