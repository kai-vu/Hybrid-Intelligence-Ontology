        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Context"/>
        <rdfs:comment xml:lang="en">Specifies the context in which an HI team operates.</rdfs:comment>
        <rdfs:label>operates in context</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#performedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#performedBy">
        <owl:inverseOf rdf:resource="https://w3id.org/hi-ontology#performsExecution"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/hi-ontology#TaskExecution"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:comment xml:lang="en">Execution performed by an agent.</rdfs:comment>
        <rdfs:label>performed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#performsExecution -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#performsExecution">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#TaskExecution"/>
        <rdfs:comment xml:lang="en">Agent that performs a specific task execution.</rdfs:comment>
        <rdfs:label>performs execution</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#realizedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#realizedBy">
