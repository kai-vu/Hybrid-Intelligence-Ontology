        <rdfs:label xml:lang="en">Method</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role -->


    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role">
        <rdfs:comment xml:lang="en">A functional or organizational position occupied by an agent within an HI team.</rdfs:comment>
        <rdfs:label xml:lang="en">Role</rdfs:label>
    </owl:Class>
    


    <!-- http://xmlns.com/foaf/0.1/Person -->


    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person"/>
    


    <!-- https://w3id.org/hi-ontology#Agent -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Agent"/>
    


    <!-- https://w3id.org/hi-ontology#AgentRelatedPhenomenon -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#AgentRelatedPhenomenon">
        <rdfs:comment xml:lang="en">Human or system phenomena affecting performance or decision-making, such as bias or feedback.</rdfs:comment>
        <rdfs:label xml:lang="en">Agent-related Phenomenon</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#AlternativeHypothesis -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#AlternativeHypothesis">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hi-ontology#Hypothesis"/>
        <rdfs:comment xml:lang="en">The hypothesis positing an effect or difference.</rdfs:comment>
        <rdfs:label xml:lang="en">Alternative Hypothesis (H1)</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#ArtificialAgent -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#ArtificialAgent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:comment xml:lang="en">An artificial or computational agent (robot, software, etc.) collaborating with human agents.</rdfs:comment>
        <rdfs:label xml:lang="en">Artificial Agent</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Bias -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Bias">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hi-ontology#AgentRelatedPhenomenon"/>
        <rdfs:comment xml:lang="en">Systematic deviation in decision-making or evaluation, exhibited by agents or datasets.</rdfs:comment>
        <rdfs:label xml:lang="en">Bias</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Capability -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Capability">
        <rdfs:comment xml:lang="en">A skill, competence, or functional capacity that allows an agent to perform or enable a task.</rdfs:comment>
        <rdfs:label xml:lang="en">Capability</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Context -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Context">
        <rdfs:comment xml:lang="en">Situational conditions (environmental, cultural, cognitive) under which tasks and evaluations occur.</rdfs:comment>
        <rdfs:label xml:lang="en">Context</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Domain -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Domain">
        <rdfs:comment xml:lang="en">The subject area or discipline in which a Use Case or research scenario belongs.</rdfs:comment>
        <rdfs:label xml:lang="en">Domain</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#EthicalConstraint -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#EthicalConstraint">
        <rdfs:comment xml:lang="en">Normative condition constraining agent behavior, tasks, or evaluations within a context.</rdfs:comment>
        <rdfs:label xml:lang="en">Ethical Constraint</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Evaluation -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Evaluation">
        <rdfs:comment xml:lang="en">A process of assessing the quality or outcome of executions, methods, or goals.</rdfs:comment>
        <rdfs:label xml:lang="en">Evaluation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Experiment -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Experiment">
        <rdfs:comment xml:lang="en">An empirical setup linked to an evaluation that tests metrics and hypotheses.</rdfs:comment>
        <rdfs:label xml:lang="en">Experiment</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Feedback -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Feedback">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hi-ontology#AgentRelatedPhenomenon"/>
        <rdfs:comment xml:lang="en">Information returned to an agent that influences subsequent actions or learning.</rdfs:comment>
        <rdfs:label xml:lang="en">Feedback</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Goal -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Goal">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hi-ontology#requiresTask"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/hi-ontology#Task"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A desired outcome pursued by an HI team, requiring one or more tasks to be completed.</rdfs:comment>
        <rdfs:label xml:lang="en">Goal</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#HITeam -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#HITeam">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hi-ontology#hasMember"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/hi-ontology#ArtificialAgent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hi-ontology#hasMember"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/hi-ontology#HumanAgent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A hybrid intelligence team composed of human and artificial agents pursuing a shared goal.</rdfs:comment>
        <rdfs:label xml:lang="en">HI Team</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#HumanAgent -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#HumanAgent">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:comment xml:lang="en">A human participant in the hybrid intelligence system, possessing cognitive and perceptual abilities.</rdfs:comment>
        <rdfs:label xml:lang="en">Human Agent</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Hypothesis -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Hypothesis">
        <rdfs:comment xml:lang="en">A proposed explanation or assumption tested by experiments.</rdfs:comment>
        <rdfs:label xml:lang="en">Hypothesis</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Intentionality -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Intentionality">
        <rdfs:comment xml:lang="en">Communicative/social intent (explaining, requesting, negotiating, instructing...).</rdfs:comment>
        <rdfs:label xml:lang="en">Interaction Intentionality</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Interaction -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Interaction">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hi-ontology#TaskExecution"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hi-ontology#performedBy"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/hi-ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A multi-agent task execution where two or more agents coordinate (verbal, symbolic, or physical) toward a goal.</rdfs:comment>
        <rdfs:label xml:lang="en">Interaction</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#InteractionModality -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#InteractionModality">
        <rdfs:comment xml:lang="en">Channel or medium of interaction (verbal dialog, gesture, symbolic messages, haptic, etc.).</rdfs:comment>
        <rdfs:label xml:lang="en">Interaction Modality</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Metric -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Metric">
        <rdfs:comment xml:lang="en">A measurable criterion used to quantify performance or outcomes in an evaluation.</rdfs:comment>
        <rdfs:label xml:lang="en">Metric</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#NullHypothesis -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#NullHypothesis">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hi-ontology#Hypothesis"/>
        <rdfs:comment xml:lang="en">The default hypothesis positing no effect or difference.</rdfs:comment>
        <rdfs:label xml:lang="en">Null Hypothesis (H0)</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#ScientificPaper -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#ScientificPaper">
        <rdfs:comment xml:lang="en">A written document describing a Use Case, domain, or experimental configuration.</rdfs:comment>
        <rdfs:label xml:lang="en">Scientific Paper</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#Task -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#Task">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hi-ontology#requiresCapability"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/hi-ontology#Capability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A unit of purposeful activity executed by an agent toward achieving a goal.</rdfs:comment>
        <rdfs:label xml:lang="en">Task</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#TaskExecution -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#TaskExecution">
        <rdfs:comment xml:lang="en">The realization or performance of a specific task by an agent using a method.</rdfs:comment>
        <rdfs:label xml:lang="en">Task Execution</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hi-ontology#UseCase -->


    <owl:Class rdf:about="https://w3id.org/hi-ontology#UseCase">
        <rdfs:comment xml:lang="en">A structured situation describing a specific hybrid intelligence scenario (previously &apos;Scenario&apos;).</rdfs:comment>
        <rdfs:label xml:lang="en">Use Case</rdfs:label>
    </owl:Class>
    


