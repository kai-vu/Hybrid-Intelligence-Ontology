    <!-- https://w3id.org/hi-ontology#usesMethod -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#usesMethod">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#TaskExecution"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method"/>
        <rdfs:comment xml:lang="en">Execution makes use of a specific method.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#usesMethodByAgent -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#usesMethodByAgent">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#performsExecution"/>
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#usesMethod"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Inferred: an agent uses a method via the executions it performs.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#usesMetric -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#usesMetric">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Evaluation"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Metric"/>
    </owl:ObjectProperty>
    


