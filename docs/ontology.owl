    <!-- https://w3id.org/hi-ontology#evaluatedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#evaluatedBy">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#TaskExecution"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Evaluation"/>
        <rdfs:comment xml:lang="en">Execution is evaluated by a defined evaluation.</rdfs:comment>
        <rdfs:label>evaluated by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#executesTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#executesTask">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hi-ontology#assignedToTask"/>
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Task"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#performsExecution"/>
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#realizesTask"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Agent executes a task (derivable from Agent→Execution→Task).</rdfs:comment>
        <rdfs:label>executes task</rdfs:label>
    </owl:ObjectProperty>
    


