<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/hi-ontology#"
     xml:base="https://w3id.org/hi-ontology"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/">
    <owl:Ontology rdf:about="https://w3id.org/hi-ontology#">
        <dc:description xml:lang="en">Conceptual model of Hybrid Intelligence: agents, teams, goals, tasks, capabilities, methods, evaluations, context, and ethical phenomena.</dc:description>
        <dc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <dc:title xml:lang="en">The Hybrid Intelligence (HI) Ontology</dc:title>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-01</terms:created>
        <vann:preferredNamespacePrefix>hi</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/hi-ontology#</vann:preferredNamespaceUri>
        <owl:versionInfo>2.0.0</owl:versionInfo>
    </owl:Ontology>
    


