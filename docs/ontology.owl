        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#TaskExecution"/>
        <rdfs:comment xml:lang="en">A task is realized through a task execution.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#realizesTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#realizesTask">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#TaskExecution"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Task"/>
        <rdfs:comment xml:lang="en">Task execution realizes a task.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#requiresCapability -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#requiresCapability">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Task"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Capability"/>
        <rdfs:comment xml:lang="en">A task requires certain capabilities to be accomplished.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#requiresTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#requiresTask">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Goal"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Task"/>
        <rdfs:comment xml:lang="en">A goal requires one or more tasks to be fulfilled.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#testsMetric -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#testsMetric">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Experiment"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Metric"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#towardsGoal -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#towardsGoal">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#TaskExecution"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Goal"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#usesInteractionModality -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#usesInteractionModality">
