    <!-- https://w3id.org/hi-ontology#allowsTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#allowsTask">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Capability"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Task"/>
        <rdfs:comment xml:lang="en">A capability enables or allows a task to be performed.</rdfs:comment>
        <rdfs:label>allows task</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#assignedToTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#assignedToTask"/>
    


    <!-- https://w3id.org/hi-ontology#eligibleForTask -->


