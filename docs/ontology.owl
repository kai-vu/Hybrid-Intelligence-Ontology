    <!-- https://w3id.org/hi-ontology#iaAssignedToTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#iaAssignedToTask">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hi-ontology#isEligibleForTask"/>
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Task"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#performsExecution"/>
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#realizesTask"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relates an agent to a task that it is formally assigned to perform. Every assignment implies that the agent is eligible for that task.</rdfs:comment>
        <rdfs:label>is assigned to task</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#isEligibleForTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#isEligibleForTask">
        <rdfs:domain rdf:resource="https://w3id.org/hi-ontology#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/hi-ontology#Task"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#hasCapability"/>
            <rdf:Description rdf:about="https://w3id.org/hi-ontology#allowsTask"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relates an agent to a task it is eligible to perform, based on their capabilities. Assignment is a stricter, policy-level decision that presupposes eligibility.</rdfs:comment>
        <rdfs:label>is eligible for task</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#isInfluencedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#isInfluencedBy">
        <owl:inverseOf rdf:resource="https://w3id.org/hi-ontology#hasInfluenceOn"/>
        <rdfs:comment xml:lang="en">Related a HI Team to the contextual condition that affects its actions, collaboration dynamics, or task performance.</rdfs:comment>
        <rdfs:label>is influenced by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hi-ontology#operatesInContext -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hi-ontology#operatesInContext">
