    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotations
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    <rdf:Description rdf:about="https://w3id.org/hi-ontology#Agent">
        <rdfs:comment xml:lang="en">An autonomous entity capable of having roles, goals, and capabilities within a hybrid intelligence system.</rdfs:comment>
        <rdfs:label xml:lang="en">Agent</rdfs:label>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Rules
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    <rdf:Description rdf:about="https://w3id.org/hi-ontology/rules/team">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/hi-ontology/rules/agent">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/hi-ontology/rules/role">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/hi-ontology/rules/goal">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/hi-ontology/rules/task">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/hi-ontology/rules/cap">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/hi-ontology/rules/exec">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description>
        <rdfs:comment>If an HI team exists and has a member agent, then the agent must be classified by some Role (hi:hasRole).</rdfs:comment>
        <rdfs:label>Rule 1 — Team membership implies having a role</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="https://w3id.org/hi-ontology#HITeam"/>
                        <swrl:argument1 rdf:resource="https://w3id.org/hi-ontology/rules/team"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="https://w3id.org/hi-ontology#hasMember"/>
                                <swrl:argument1 rdf:resource="https://w3id.org/hi-ontology/rules/team"/>
                                <swrl:argument2 rdf:resource="https://w3id.org/hi-ontology/rules/agent"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </rdf:Description>
                </rdf:rest>
            </rdf:Description>
        </swrl:body>
        <swrl:head>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                        <swrl:propertyPredicate rdf:resource="https://w3id.org/hi-ontology#hasRole"/>
                        <swrl:argument1 rdf:resource="https://w3id.org/hi-ontology/rules/agent"/>
                        <swrl:argument2 rdf:resource="https://w3id.org/hi-ontology/rules/role"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>
    <rdf:Description>
        <rdfs:comment>If team has goal G requiring task T, and agent ∈ team and has capability C that allows T ⇒ agent eligible for T.</rdfs:comment>
        <rdfs:label>Rule 2 — Team, Goal, and Capability determine eligibility</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="https://w3id.org/hi-ontology#HITeam"/>
                        <swrl:argument1 rdf:resource="https://w3id.org/hi-ontology/rules/team"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="https://w3id.org/hi-ontology#hasGoal"/>
                                <swrl:argument1 rdf:resource="https://w3id.org/hi-ontology/rules/team"/>
                                <swrl:argument2 rdf:resource="https://w3id.org/hi-ontology/rules/goal"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                        <swrl:propertyPredicate rdf:resource="https://w3id.org/hi-ontology#requiresTask"/>
                                        <swrl:argument1 rdf:resource="https://w3id.org/hi-ontology/rules/goal"/>
                                        <swrl:argument2 rdf:resource="https://w3id.org/hi-ontology/rules/task"/>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                <swrl:propertyPredicate rdf:resource="https://w3id.org/hi-ontology#hasMember"/>
                                                <swrl:argument1 rdf:resource="https://w3id.org/hi-ontology/rules/team"/>
                                                <swrl:argument2 rdf:resource="https://w3id.org/hi-ontology/rules/agent"/>
                                            </rdf:Description>
                                        </rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                <rdf:first>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                        <swrl:propertyPredicate rdf:resource="https://w3id.org/hi-ontology#hasCapability"/>
                                                        <swrl:argument1 rdf:resource="https://w3id.org/hi-ontology/rules/agent"/>
                                                        <swrl:argument2 rdf:resource="https://w3id.org/hi-ontology/rules/cap"/>
                                                    </rdf:Description>
                                                </rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                        <rdf:first>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                                <swrl:propertyPredicate rdf:resource="https://w3id.org/hi-ontology#allowsTask"/>
                                                                <swrl:argument1 rdf:resource="https://w3id.org/hi-ontology/rules/cap"/>
                                                                <swrl:argument2 rdf:resource="https://w3id.org/hi-ontology/rules/task"/>
                                                            </rdf:Description>
                                                        </rdf:first>
                                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </rdf:rest>
            </rdf:Description>
        </swrl:body>
        <swrl:head>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                        <swrl:propertyPredicate rdf:resource="https://w3id.org/hi-ontology#eligibleForTask"/>
                        <swrl:argument1 rdf:resource="https://w3id.org/hi-ontology/rules/agent"/>
                        <swrl:argument2 rdf:resource="https://w3id.org/hi-ontology/rules/task"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>
    <rdf:Description>
        <rdfs:comment>If an agent is assigned to a task and an execution realizes that task, then that execution is performed by the agent.</rdfs:comment>
        <rdfs:label>Rule 4 — Assignment → Performance</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                        <swrl:propertyPredicate rdf:resource="https://w3id.org/hi-ontology#assignedToTask"/>
                        <swrl:argument1 rdf:resource="https://w3id.org/hi-ontology/rules/agent"/>
                        <swrl:argument2 rdf:resource="https://w3id.org/hi-ontology/rules/task"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
