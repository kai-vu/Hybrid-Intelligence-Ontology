@prefix hi:       <https://w3id.org/hi-ontology#> .
@prefix dc:       <http://purl.org/dc/elements/1.1/> .
@prefix dul:      <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix owl:      <http://www.w3.org/2002/07/owl#> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml:      <http://www.w3.org/XML/1998/namespace> .
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf:     <http://xmlns.com/foaf/0.1/> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl:     <http://www.w3.org/2003/11/swrl#> .
@prefix vann:     <http://purl.org/vocab/vann/> .
@prefix swrla:    <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix schema:   <http://schema.org/> .
@prefix dcterms:  <http://purl.org/dc/terms/> .


<https://w3id.org/hi-ontology#>  a owl:Ontology ;
    dc:title "The Hybrid Intelligence (HI) Ontology"@en ;
    dc:description "Conceptual model of Hybrid Intelligence: agents, teams, goals, tasks, capabilities, methods, evaluations, context, and ethical phenomena."@en ;
    dc:creator [
        a foaf:Person ;
        foaf:name "Francesca Urgese" ;
        schema:identifier [
            a schema:PropertyValue ;
            schema:propertyID "ORCID" ;
            schema:value "0009-0009-0516-1800";
            foaf:member _:org  
        ]], [
        a foaf:Person ;
        foaf:name "Margherita Martorana" ;
        schema:identifier [
            a schema:PropertyValue ;
            schema:propertyID "ORCID" ;
            schema:value "0000-0001-8004-0464" ;
            foaf:member _:org  
        ]] , [
        a foaf:Person ;
        foaf:name "Ilaria Tiddi" ;
        schema:identifier [
            a schema:PropertyValue ;
            schema:propertyID "ORCID" ;
            schema:value "0000-0001-7116-9338" ;
            foaf:member _:org  
        ]] ;
    dcterms:created "2025-10-01"^^xsd:date ;
    owl:versionInfo "2.0.0" ;
    dc:license <https://creativecommons.org/licenses/by/4.0/> ;
    vann:preferredNamespacePrefix "hi";
    vann:preferredNamespaceUri "https://w3id.org/hi-ontology#" .

_:org a schema:Organization ;
    schema:name "Knowledge in AI (KAI) Group - Vrije Universiteit Amsterdam" .


#################################################################
# HI Object Properties
#################################################################

###  https://w3id.org/hi-ontology#hasUseCaseConcept
hi:hasUseCaseConcept a owl:ObjectProperty ;
    rdfs:domain hi:UseCase ;
    rdfs:range  skos:Concept ;
    rdfs:label "has use case concept" ;
    rdfs:comment "Links a use case to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#hasDomainConcept
hi:hasDomainConcept a owl:ObjectProperty ;
    rdfs:domain hi:UseCase ;
    rdfs:range  skos:Concept ;
    rdfs:label "has domain concept" ;
    rdfs:comment "Links a domain to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#hasHITeam
hi:hasHITeam a owl:ObjectProperty ;
    rdfs:domain hi:UseCase ;
    rdfs:range hi:HITeam ;
    rdfs:label "has HI team" ;
    rdfs:comment "A use case introduces a hybrid intelligence team."@en .

###  https://w3id.org/hi-ontology#hasGoal
hi:hasGoal a owl:ObjectProperty ;
    rdfs:domain hi:HITeam ;
    rdfs:range hi:Goal ;
    rdfs:label "has goal" ;
    rdfs:comment "An HI team pursues one or more goals."@en .

###  https://w3id.org/hi-ontology#hasMember
hi:hasMember a owl:ObjectProperty ;
    rdfs:domain hi:HITeam ;
    rdfs:range hi:Agent ;
    rdfs:label "has member" ;
    rdfs:comment "Specifies agents that are members of an HI team."@en .

###  https://w3id.org/hi-ontology#operatesInContext
hi:operatesInContext a owl:ObjectProperty ;
    rdfs:domain hi:Agent ;
    rdfs:range hi:Context ;
    rdfs:label "operates in context" ;
    rdfs:comment "Specifies the context in which an HI team operates."@en .

###  https://w3id.org/hi-ontology#hasConstraintConcept
hi:hasConstraintConcept a owl:ObjectProperty ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( hi:Context hi:HITeam ) 
    ] ;
    rdfs:range skos:Concept ;
    rdfs:label "has constraint concept" ;
    rdfs:comment "Specifies the constraint using a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#hasGoalConcept
hi:hasGoalConcept a owl:ObjectProperty ;
    rdfs:domain hi:Goal ;
    rdfs:range skos:Concept ;
    rdfs:label "has goal concept" ;
    rdfs:comment "Links a goal to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#hasTask
hi:hasTask a owl:ObjectProperty ;
    rdfs:domain hi:Goal ;
    rdfs:range hi:Task ;
    rdfs:label "has task" ;
    rdfs:comment "A goal requires one or more tasks to be fulfilled."@en .

###  https://w3id.org/hi-ontology#hasContextConcept
hi:hasContextConcept a owl:ObjectProperty ;
    rdfs:domain hi:Context ;
    rdfs:range  skos:Concept ;
    rdfs:label "has context concept" ;
    rdfs:comment "Links a context to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#exhibitsPhenomenonConcept
hi:exhibitsPhenomenonConcept a owl:ObjectProperty ;
    rdfs:domain hi:Context ;
    rdfs:range skos:Concept ;
    rdfs:label "exhibits phenomenon concept" ;
    rdfs:comment "Context exhibits a phenomenon (e.g., bias/feedback emerges in that context)."@en .

###  https://w3id.org/hi-ontology#hasRoleConcept
hi:hasRoleConcept a owl:ObjectProperty ;
    rdfs:domain hi:Agent ;
    rdfs:range  skos:Concept ;
    rdfs:comment "Links a role to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#hasCapability
hi:hasCapability a owl:ObjectProperty ;
    rdfs:domain hi:Agent ;
    rdfs:range hi:Capability ;
    rdfs:label "has capability" ;
    rdfs:comment "Agent possesses a capability."@en .

###  https://w3id.org/hi-ontology#performsExecution
hi:performsExecution a owl:ObjectProperty ;
    rdfs:domain hi:Agent ;
    rdfs:range hi:TaskExecution ;
    rdfs:label "performs execution" ;
    rdfs:comment "Agent that performs a specific task execution."@en .

###  https://w3id.org/hi-ontology#hasCapabilityConcept
hi:hasCapabilityConcept a owl:ObjectProperty ;
    rdfs:domain hi:Capability ;
    rdfs:range  skos:Concept ;
    rdfs:label "has capability concept" ;
    rdfs:comment "Links a capability to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#allowsTask
hi:allowsTask a owl:ObjectProperty ;
    rdfs:domain hi:Capability ;
    rdfs:range hi:Task ;
    rdfs:label "allows task" ;
    rdfs:comment "A capability enables or allows a task to be performed."@en .

### https://w3id.org/hi-ontology#hasTaskConcept
hi:hasTaskConcept a owl:ObjectProperty ;
    rdfs:domain hi:Task ;
    rdfs:range  skos:Concept ;
    rdfs:comment "Links a task to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#requiresCapability
hi:requiresCapability a owl:ObjectProperty ;
    rdfs:domain hi:Task ;
    rdfs:range hi:Capability ;
    rdfs:label "requires cability" ;
    rdfs:comment "A task requires certain capabilities to be accomplished."@en .

### https://w3id.org/hi-ontology#hasExecutionConcept
hi:hasExecutionConcept a owl:ObjectProperty ;
    rdfs:domain hi:Execution ;
    rdfs:range  skos:Concept ;
    rdfs:label "has execution concept" ;
    rdfs:comment "Links an experiment to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#realizesTask
hi:realizesTask a owl:ObjectProperty ;
    rdfs:domain hi:TaskExecution ;
    rdfs:range hi:Task ;
    rdfs:label "realizes task" ;
    rdfs:comment "Task execution realizes a task."@en .

###  https://w3id.org/hi-ontology#performedBy
hi:performedBy a owl:ObjectProperty ;
    owl:inverseOf hi:performsExecution ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( hi:TaskExecution hi:Interaction ) 
    ] ;
    rdfs:range hi:Agent ;
    rdfs:label "performed by" ;
    rdfs:comment "Execution performed by an agent."@en .

###  https://w3id.org/hi-ontology#towardsGoal
hi:towardsGoal a owl:ObjectProperty ;
    rdfs:domain hi:TaskExecution ;
    rdfs:range hi:Goal ;
    rdfs:label "towards goal" ;
    rdfs:comment "Task execution towards a goal."@en .

###  https://w3id.org/hi-ontology#usesMethod
hi:usesMethod a owl:ObjectProperty ;
    rdfs:domain hi:TaskExecution ;
    rdfs:range dul:Method ;
    rdfs:label "uses method" ;
    rdfs:comment "Execution makes use of a specific method."@en .

### https://w3id.org/hi-ontology#hasMethodConcept
hi:hasMethodConcept a owl:ObjectProperty ;
    rdfs:domain dul:Method ;
    rdfs:range  skos:Concept ;
    rdfs:label "has method concept" ;
    rdfs:comment "Links a method to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#hasInteractionIntentConcept
hi:hasInteractionIntentConcept a owl:ObjectProperty ;
    rdfs:domain hi:Interaction ;
    rdfs:range hi:Intentionality ;
    rdfs:label "has interaction intent concept" ;
    rdfs:comment "Intent underlying the interaction (e.g., explain, request, negotiate)."@en .

###  https://w3id.org/hi-ontology#hasInteractionModalityConcept
hi:hasInteractionModalityConcept a owl:ObjectProperty ;
    rdfs:domain hi:Interaction ;
    rdfs:range skos:Concept ;
    rdfs:label "has interaction modality concept" ;
    rdfs:comment "Modality/channel used during the interaction."@en .

###  https://w3id.org/hi-ontology#influencedByFeedbackConcept
hi:influencedByFeedbackConcept a owl:ObjectProperty ;
    rdfs:domain hi:Agent ; 
    rdfs:range skos:Concept ;
    rdfs:label "is influenced by" ;
    rdfs:comment "An agent is influenced by a feedback concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#evaluatedBy
hi:evaluatedBy a owl:ObjectProperty ;
    rdfs:domain hi:TaskExecution ;
    rdfs:range hi:Evaluation ;
    rdfs:label "evaluated by" ;
    rdfs:comment "Execution is evaluated by a defined evaluation."@en .

###  https://w3id.org/hi-ontology#hasEvaluationConcept
hi:hasEvaluationConcept a owl:ObjectProperty ;
    rdfs:domain hi:Evaluation ;
    rdfs:range  skos:Concept ;
    rdfs:label "has evaluation concept" ;
    rdfs:comment "Links an evaluation to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#usesMetric
hi:usesMetric a owl:ObjectProperty ;
    rdfs:domain hi:Evaluation ;
    rdfs:range hi:Metric ;
    rdfs:label "uses metric" ;
    rdfs:comment "An evaluation uses certain metrics."@en .

###  https://w3id.org/hi-ontology#hasExperiment
hi:hasExperiment a owl:ObjectProperty ;
    rdfs:domain hi:Evaluation ;
    rdfs:range hi:Experiment ;
    rdfs:label "has experiment" ;
    rdfs:comment "An evaluation has an experiment."@en .

### https://w3id.org/hi-ontology#hasExperimentConcept
hi:hasExperimentConcept a owl:ObjectProperty ;
    rdfs:domain hi:Experiment ;
    rdfs:range  skos:Concept ;
    rdfs:label "has execution concept" ;
    rdfs:comment "Links an experiment to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#testsMetric
hi:testsMetric a owl:ObjectProperty ;
    rdfs:domain hi:Experiment ;
    rdfs:range hi:Metric ;
    rdfs:label "tests metric" ;
    rdfs:comment "An experiment tests certain metrics."@en .

###  https://w3id.org/hi-ontology#hasNullHypothesis
hi:hasNullHypothesis a owl:ObjectProperty ;
    rdfs:domain hi:Experiment ;
    rdfs:range hi:NullHypothesis ;
    rdfs:label "has null hypothesis" ;
    rdfs:comment "An experiment has a null hypothesis."@en .

###  https://w3id.org/hi-ontology#hasAlternativeHypothesis
hi:hasAlternativeHypothesis a owl:ObjectProperty ;
    rdfs:domain hi:Evaluation ;
    rdfs:range hi:AlternativeHypothesis ;
    rdfs:label "has alternative hypothesis" ;
    rdfs:comment "An experiment has an alternative hypothesis."@en .

###  https://w3id.org/hi-ontology#hasMetricConcept
hi:hasMetricConcept a owl:ObjectProperty ;
    rdfs:domain hi:Metric ;
    rdfs:range  skos:Concept ;
    rdfs:comment "Links a metric to a concept in the HINT thesaurus."@en . 

###  https://w3id.org/hi-ontology#hasAgentConcept
hi:hasAgentConcept a owl:ObjectProperty ;
    rdfs:domain hi:Agent ;
    rdfs:range  skos:Concept ;
    rdfs:comment "Links an agent to a concept in the HINT thesaurus."@en . 

###  https://w3id.org/hi-ontology#assignedToTask
hi:assignedToTask a owl:ObjectProperty ;
    rdfs:domain hi:Agent ;
    rdfs:range hi:Task ;
    owl:propertyChainAxiom ( hi:performsExecution hi:realizesTask ) ;
    rdfs:label "assigned to task" ; 
    rdfs:comment "Agent formally assigned to perform a task (inferred from executions)."@en .

###  https://w3id.org/hi-ontology#eligibleForTask
hi:eligibleForTask a owl:ObjectProperty ;
    rdfs:subPropertyOf hi:assignedToTask ;
    rdfs:domain hi:Agent ;
    rdfs:range hi:Task ;
    rdfs:label "eligible for task" ;
    rdfs:comment "Agent eligible for a task based on membership and capabilities (policy-level notion)."@en .

###  https://w3id.org/hi-ontology#executesTask
hi:executesTask a owl:ObjectProperty ;
    rdfs:subPropertyOf hi:assignedToTask ;
    rdfs:domain hi:Agent ;
    rdfs:range hi:Task ;
    owl:propertyChainAxiom ( hi:performsExecution hi:realizesTask ) ;
    rdfs:label "executes task" ;
    rdfs:comment "Agent executes a task (derivable from Agent→Execution→Task)."@en .

###  https://w3id.org/hi-ontology#hasBiasConcept
hi:hasBiasConcept a owl:ObjectProperty ;
    rdfs:domain hi:TaskExecution ;
    rdfs:range  skos:Concept ;
    rdfs:label "has bias concept" ;
    rdfs:comment "Links a bias to a concept in the HINT thesaurus."@en .

###  https://w3id.org/hi-ontology#realizedBy
hi:realizedBy a owl:ObjectProperty ;
    owl:inverseOf hi:realizesTask ;
    rdfs:domain hi:Task ;
    rdfs:range hi:TaskExecution ;
    rdfs:label "realized by" ;
    rdfs:comment "A task is realized through a task execution."@en .




#################################################################
# HI Classes
#################################################################

###  https://w3id.org/hi-ontology#UseCase
hi:UseCase a owl:Class ;
    rdfs:comment "A structured situation describing a specific hybrid intelligence scenario (previously 'Scenario')."@en ;
    rdfs:label "Use Case"@en .

###  https://w3id.org/hi-ontology#HITeam
hi:HITeam a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
                      owl:onProperty hi:hasMember ;
                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                      owl:onClass hi:ArtificialAgent
                    ] ,
                    [ a owl:Restriction ;
                      owl:onProperty hi:hasMember ;
                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                      owl:onClass hi:HumanAgent
                    ] ;
    rdfs:comment "A hybrid intelligence team composed of human and artificial agents pursuing a shared goal."@en ;
    rdfs:label "HI Team"@en .

###  https://w3id.org/hi-ontology#Goal
hi:Goal a owl:Class ;
    rdfs:subClassOf dul:Goal ,
        [ a owl:Restriction ;
          owl:onProperty hi:requiresTask ;
          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onClass hi:Task
        ] ;
    rdfs:comment "A desired outcome pursued by an HI team, requiring one or more tasks to be completed."@en ;
    rdfs:label "Goal"@en .

###  https://w3id.org/hi-ontology#Context
hi:Context a owl:Class ;
    rdfs:comment "Situational conditions (environmental, cultural, cognitive) under which tasks and evaluations occur."@en ;
    rdfs:label "Context"@en .

###  https://w3id.org/hi-ontology#Agent
hi:Agent a owl:Class, dul:Entity ;
    rdfs:comment "An autonomous entity capable of having roles, goals, and capabilities within a hybrid intelligence system."@en ;
    rdfs:label "Agent"@en .

###  https://w3id.org/hi-ontology#ArtificialAgent
hi:ArtificialAgent a owl:Class ;
    rdfs:subClassOf hi:Agent ;
    rdfs:comment "An artificial or computational agent (robot, software, etc.) collaborating with human agents."@en ;
    rdfs:label "Artificial Agent"@en .

###  https://w3id.org/hi-ontology#HumanAgent
hi:HumanAgent a owl:Class ;
    rdfs:subClassOf hi:Agent , foaf:Person ;
    rdfs:comment "A human participant in the hybrid intelligence system, possessing cognitive and perceptual abilities."@en ;
    rdfs:label "Human Agent"@en .

###  https://w3id.org/hi-ontology#Capability
hi:Capability a owl:Class ;
    rdfs:comment "A skill, competence, or functional capacity that allows an agent to perform or enable a task."@en ;
    rdfs:label "Capability"@en .

###  https://w3id.org/hi-ontology#Task
hi:Task a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
                      owl:onProperty hi:requiresCapability ;
                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                      owl:onClass hi:Capability
                    ] ;
    rdfs:comment "A unit of purposeful activity executed by an agent toward achieving a goal."@en ;
    rdfs:label "Task"@en .

###  https://w3id.org/hi-ontology#TaskExecution
hi:TaskExecution a owl:Class ;
    rdfs:comment "The realization or performance of a specific task by an agent using a method."@en ;
    rdfs:label "Task Execution"@en .

###  https://w3id.org/hi-ontology#Interaction
hi:Interaction a owl:Class ;
    rdfs:subClassOf hi:TaskExecution ,
                    [ a owl:Restriction ;
                      owl:onProperty hi:performedBy ;
                      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                      owl:onClass hi:Agent
                    ] ;
    rdfs:comment "A multi-agent task execution where two or more agents coordinate (verbal, symbolic, or physical) toward a goal."@en ;
    rdfs:label "Interaction"@en .

###  https://w3id.org/hi-ontology#Evaluation
hi:Evaluation a owl:Class ;
    rdfs:comment "A process of assessing the quality or outcome of executions, methods, or goals."@en ;
    rdfs:label "Evaluation"@en .

###  https://w3id.org/hi-ontology#Experiment
hi:Experiment a owl:Class ;
    rdfs:comment "An empirical setup linked to an evaluation that tests metrics and hypotheses."@en ;
    rdfs:label "Experiment"@en .

###  https://w3id.org/hi-ontology#Hypothesis
hi:Hypothesis a owl:Class ;
    rdfs:comment "A proposed explanation or assumption tested by experiments."@en ;
    rdfs:label "Hypothesis"@en .

###  https://w3id.org/hi-ontology#AlternativeHypothesis
hi:AlternativeHypothesis a owl:Class ;
    rdfs:subClassOf hi:Hypothesis ;
    rdfs:comment "The hypothesis positing an effect or difference."@en ;
    rdfs:label "Alternative Hypothesis (H1)"@en .

###  https://w3id.org/hi-ontology#NullHypothesis
hi:NullHypothesis a owl:Class ;
    rdfs:subClassOf hi:Hypothesis ;
    rdfs:comment "The default hypothesis positing no effect or difference."@en ;
    rdfs:label "Null Hypothesis (H0)"@en .

###  https://w3id.org/hi-ontology#Metric
hi:Metric a owl:Class ;
    rdfs:comment "A measurable criterion used to quantify performance or outcomes in an evaluation."@en ;
    rdfs:label "Metric"@en .




#################################################################
# Reused Classes
#################################################################

###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Method
dul:Method a owl:Class ;
    rdfs:comment "A technical or procedural means used during task execution or evaluation."@en ;
    rdfs:label "Method"@en .

###  http://xmlns.com/foaf/0.1/Person
foaf:Person a owl:Class .




#################################################################
# HI Rules
#################################################################

<https://w3id.org/hi-ontology/rules/team> a swrl:Variable .

<https://w3id.org/hi-ontology/rules/goal> a swrl:Variable .

<https://w3id.org/hi-ontology/rules/task> a swrl:Variable .

<https://w3id.org/hi-ontology/rules/agent> a swrl:Variable .

<https://w3id.org/hi-ontology/rules/cap> a swrl:Variable .

<https://w3id.org/hi-ontology/rules/role> a swrl:Variable .

<https://w3id.org/hi-ontology/rules/exec> a swrl:Variable .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "If team has goal G requiring task T, and agent ∈ team and has capability C that allows T ⇒ agent eligible for T." ;
   rdfs:label "Rule 2 — Team, Goal, and Capability determine eligibility" ;
   a swrl:Imp ;
   swrl:body [ a swrl:AtomList ;
               rdf:first [ a swrl:ClassAtom ;
                           swrl:classPredicate hi:HITeam ;
                           swrl:argument1 <https://w3id.org/hi-ontology/rules/team>
                         ] ;
               rdf:rest [ a swrl:AtomList ;
                          rdf:first [ a swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate hi:hasGoal ;
                                      swrl:argument1 <https://w3id.org/hi-ontology/rules/team> ;
                                      swrl:argument2 <https://w3id.org/hi-ontology/rules/goal>
                                    ] ;
                          rdf:rest [ a swrl:AtomList ;
                                     rdf:first [ a swrl:IndividualPropertyAtom ;
                                                 swrl:propertyPredicate hi:requiresTask ;
                                                 swrl:argument1 <https://w3id.org/hi-ontology/rules/goal> ;
                                                 swrl:argument2 <https://w3id.org/hi-ontology/rules/task>
                                               ] ;
                                     rdf:rest [ a swrl:AtomList ;
                                                rdf:first [ a swrl:IndividualPropertyAtom ;
                                                            swrl:propertyPredicate hi:hasMember ;
                                                            swrl:argument1 <https://w3id.org/hi-ontology/rules/team> ;
                                                            swrl:argument2 <https://w3id.org/hi-ontology/rules/agent>
                                                          ] ;
                                                rdf:rest [ a swrl:AtomList ;
                                                           rdf:first [ a swrl:IndividualPropertyAtom ;
                                                                       swrl:propertyPredicate hi:hasCapability ;
                                                                       swrl:argument1 <https://w3id.org/hi-ontology/rules/agent> ;
                                                                       swrl:argument2 <https://w3id.org/hi-ontology/rules/cap>
                                                                     ] ;
                                                           rdf:rest [ a swrl:AtomList ;
                                                                      rdf:first [ a swrl:IndividualPropertyAtom ;
                                                                                  swrl:propertyPredicate hi:allowsTask ;
                                                                                  swrl:argument1 <https://w3id.org/hi-ontology/rules/cap> ;
                                                                                  swrl:argument2 <https://w3id.org/hi-ontology/rules/task>
                                                                                ] ;
                                                                      rdf:rest rdf:nil
                                                                    ]
                                                         ]
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ a swrl:AtomList ;
               rdf:first [ a swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate hi:eligibleForTask ;
                           swrl:argument1 <https://w3id.org/hi-ontology/rules/agent> ;
                           swrl:argument2 <https://w3id.org/hi-ontology/rules/task>
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "If an HI team exists and has a member agent, then the agent must be classified by some Role (hi:hasRole)." ;
   rdfs:label "Rule 1 — Team membership implies having a role" ;
   a swrl:Imp ;
   swrl:body [ a swrl:AtomList ;
               rdf:first [ a swrl:ClassAtom ;
                           swrl:classPredicate hi:HITeam ;
                           swrl:argument1 <https://w3id.org/hi-ontology/rules/team>
                         ] ;
               rdf:rest [ a swrl:AtomList ;
                          rdf:first [ a swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate hi:hasMember ;
                                      swrl:argument1 <https://w3id.org/hi-ontology/rules/team> ;
                                      swrl:argument2 <https://w3id.org/hi-ontology/rules/agent>
                                    ] ;
                          rdf:rest rdf:nil
                        ]
             ] ;
   swrl:head [ a swrl:AtomList ;
               rdf:first [ a swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate hi:hasRole ;
                           swrl:argument1 <https://w3id.org/hi-ontology/rules/agent> ;
                           swrl:argument2 <https://w3id.org/hi-ontology/rules/role>
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "If an agent is assigned to a task and an execution realizes that task, then that execution is performed by the agent." ;
   rdfs:label "Rule 4 — Assignment → Performance" ;
   a swrl:Imp ;
   swrl:body [ a swrl:AtomList ;
               rdf:first [ a swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate hi:assignedToTask ;
                           swrl:argument1 <https://w3id.org/hi-ontology/rules/agent> ;
                           swrl:argument2 <https://w3id.org/hi-ontology/rules/task>
                         ] ;
               rdf:rest [ a swrl:AtomList ;
                          rdf:first [ a swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate hi:realizedBy ;
                                      swrl:argument1 <https://w3id.org/hi-ontology/rules/task> ;
                                      swrl:argument2 <https://w3id.org/hi-ontology/rules/exec>
                                    ] ;
                          rdf:rest rdf:nil
                        ]
             ] ;
   swrl:head [ a swrl:AtomList ;
               rdf:first [ a swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate hi:performedBy ;
                           swrl:argument1 <https://w3id.org/hi-ontology/rules/exec> ;
                           swrl:argument2 <https://w3id.org/hi-ontology/rules/agent>
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "If an agent is eligible for a task, then the agent can be assigned to that task." ;
   rdfs:label "Rule 3 — Eligibility implies assignment" ;
   a swrl:Imp ;
   swrl:body [ a swrl:AtomList ;
               rdf:first [ a swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate hi:eligibleForTask ;
                           swrl:argument1 <https://w3id.org/hi-ontology/rules/agent> ;
                           swrl:argument2 <https://w3id.org/hi-ontology/rules/task>
                         ] ;
               rdf:rest rdf:nil
             ] ;
   swrl:head [ a swrl:AtomList ;
               rdf:first [ a swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate hi:assignedToTask ;
                           swrl:argument1 <https://w3id.org/hi-ontology/rules/agent> ;
                           swrl:argument2 <https://w3id.org/hi-ontology/rules/task>
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

