#################################################################

hi-s3:UseCaseGroupDeliberationHI a hi:UseCase ;
    hi:hasUseCaseConcept hint:GroupDeliberationUseCase ;
    hi:hasDomainConcept hint:LawAndPolicyDomain ;
    hi:hasHITeam hi-s3:HITeamGroupDeliberation .

#################################################################
# HI Team, Goal, Context, Ethical Constraints
#################################################################

hi-s3:HITeamGroupDeliberation a hi:HITeam ;
    hi:hasGoal hi-s3:GoalFairAccurateVerdict ;
    hi:hasMember 
        hi-s3:AIDeliberationAssistant ,
        hi-s3:HumanJuror1 ,
        hi-s3:HumanJuror2 ;
    hi:operatesInContext hi-s3:ContextJuryDeliberation .

hi-s3:GoalFairAccurateVerdict a hi:Goal ;
    hi:hasGoalConcept hint:FairAccurateVerdictGoal ;
