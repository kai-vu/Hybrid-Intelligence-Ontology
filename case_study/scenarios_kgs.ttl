hi-s3:ExecProvideStatement2 a hi:TaskExecution ;
    hi:hasExecutionConcept hint:ProvideStatementTaskExecution ;
    hi:realizesTask hi-s3:TaskProvideStatement ;
    hi:performedBy hi-s3:HumanJuror2 ;
    hi:towardsGoal hi-s3:GoalFairAccurateVerdict .

#################################################################
# Interaction Episodes
#################################################################

hi-s3:InteractionGroupDeliberation a hi:Interaction ;
    hi:hasAgentInvolved 
        hi-s3:AIDeliberationAssistant ,
        hi-s3:HumanJuror1 ,
        hi-s3:HumanJuror2 ;
    hi:hasInteractionIntentConcept hint:DeliberationIntent ;
    hi:hasInteractionModalityConcept hint:VerbalDialogueModality .

hi-s3:InteractionExplainConditions a hi:Interaction ;
    hi:hasAgentInvolved 
        hi-s3:AIDeliberationAssistant ,
        hi-s3:HumanJuror1 ,
