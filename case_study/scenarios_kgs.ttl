#################################################################
# Task Executions 
#################################################################

hi-s3:ExecAssessAccuracy a hi:TaskExecution ;
    hi:hasExecutionConcept hint:AssessAccuracyTaskExecution ;
    hi:realizesTask hi-s3:TaskAssessCommunicationAccuracy ;
    hi:performedBy hi-s3:AIDeliberationAssistant ;
    hi:hasMethodConcept hint:BayesianLogicMethod ;
    hi:towardsGoal hi-s3:GoalFairAccurateVerdict .

hi-s3:ExecDecideIntervention a hi:TaskExecution ;
    hi:hasExecutionConcept hint:DecideInterventionTaskExecution ;
    hi:realizesTask hi-s3:TaskDecideIntervention ;
    hi:performedBy hi-s3:AIDeliberationAssistant ;
    hi:hasMethodConcept hint:MetaAnalysisScoringMethod ;
    hi:towardsGoal hi-s3:GoalFairAccurateVerdict .

hi-s3:ExecExplainDecision a hi:TaskExecution ;
    hi:hasExecutionConcept hint:ExplanationTaskExecution ;
    hi:realizesTask hi-s3:TaskExplainDecision ;
    hi:performedBy hi-s3:AIDeliberationAssistant ;
    hi:towardsGoal hi-s3:GoalFairAccurateVerdict .

hi-s3:ExecProvideStatement1 a hi:TaskExecution ;
    hi:hasExecutionConcept hint:ProvideStatementTaskExecution ;
    hi:realizesTask hi-s3:TaskProvideStatement ;
    hi:performedBy hi-s3:HumanJuror1 ;
