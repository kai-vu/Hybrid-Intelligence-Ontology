        hi-s3:TaskAssessCommunicationAccuracy ,
        hi-s3:TaskDecideIntervention ,
        hi-s3:TaskExplainDecision ,
        hi-s3:TaskProvideStatement .

hi-s3:ContextJuryDeliberation a hi:Context ;
    hi:hasContextConcept hint:JuryDeliberationContext ;
    hi:hasPhenomenonConcept hint:GroupThinkingPhenomenon ,
                             hint:TrustShiftPhenomenon ;
    hi:hasConstraintConcept 
        hint:UndueInfluenceConstraint ,
        hint:FairnessConstraint ,
        hint:TransparencyConstraint ;
	hi:hasInfluenceOn hi-s3:HITeamGroupDeliberation .


#################################################################
# Agents
#################################################################

hi-s3:AIDeliberationAssistant a hi:ArtificialAgent ;
    hi:hasAgentConcept hint:ArtificialAgent ;
    hi:hasRoleConcept hint:AIAdvisorRole ;
    hi:hasCapability 
        hi-s3:CapabilityBayesianReasoning ,
        hi-s3:CapabilityMetaAnalysis ,
        hi-s3:CapabilityExplanation ;
    hi:performsExecution 
        hi-s3:ExecAssessAccuracy ,
        hi-s3:ExecDecideIntervention ,
        hi-s3:ExecExplainDecision .

hi-s3:HumanJuror1 a hi:HumanAgent ;
    hi:hasAgentConcept hint:HumanAgent ;
    hi:hasRoleConcept hint:JurorRole ;
    hi:hasCapability hi-s3:CapabilitySelfReporting ;
    hi:performsExecution hi-s3:ExecProvideStatement1 .

hi-s3:HumanJuror2 a hi:HumanAgent ;
    hi:hasAgentConcept hint:HumanAgent ;
    hi:hasRoleConcept hint:JurorRole ;
    hi:hasCapability hi-s3:CapabilitySelfReporting ;
    hi:performsExecution hi-s3:ExecProvideStatement2 .

#################################################################
# Capabilities
#################################################################

hi-s3:CapabilityBayesianReasoning a hi:Capability ;
    hi:hasCapabilityConcept hint:BayesianReasoningCapability ;
    hi:allowsTask hi-s3:TaskAssessCommunicationAccuracy ,
                  hi-s3:TaskDecideIntervention .

hi-s3:CapabilityMetaAnalysis a hi:Capability ;
    hi:hasCapabilityConcept hint:MetaAnalysisCapability ;
    hi:allowsTask hi-s3:TaskAssessCommunicationAccuracy .

hi-s3:CapabilityExplanation a hi:Capability ;
    hi:hasCapabilityConcept hint:ExplainingCapability ;
