#################################################################
# USE CASE 1 â€” Personal Assistant Hybrid Intelligence System
#################################################################

############################
# Use Case
############################

hi-s1:UseCasePersonalAssistantHI a hi:UseCase ;
    hi:hasUseCaseConcept hint:PersonalAssistantUseCase ;
    hi:hasDomainConcept hint:HealthAndLifestyleDomain ;
    hi:hasHITeam hi-s1:HITeamPersonalAssistant .


############################
# HI Team, Context, Goal
############################

hi-s1:HITeamPersonalAssistant a hi:HITeam ;
    hi:hasGoal hi-s1:GoalUserWellbeing ;
    hi:hasMember hi-s1:AIAssistant , hi-s1:HumanUser ;
    hi:operatesInContext hi-s1:ContextHealthMonitoring .

hi-s1:ContextHealthMonitoring a hi:Context ;
    hi:hasContextConcept hint:HealthMonitoringContext ;
    hi:hasPhenomenonConcept hint:SensorNoisePhenomenon ,
                            hint:DataMismatchPhenomenon ,
                            hint:TrustShiftPhenomenon ;
    hi:hasConstraintConcept hint:PrivacyConstraint ,
                            hint:AutonomyConstraint ,
                            hint:TransparencyConstraint ;
    hi:hasInfluenceOn hi-s1:HITeamPersonalAssistant ;
    rdfs:comment "Health-monitoring context involving multimodal data, user reports, and sensor uncertainty."@en .

hi-s1:GoalUserWellbeing a hi:Goal ;
    hi:hasGoalConcept hint:UserWellbeingGoal ;
    hi:hasTask hi-s1:TaskDataCollection ,
               hi-s1:TaskConflictResolution ,
               hi-s1:TaskExplanation ;
    rdfs:comment "Maintain and improve user wellbeing using data fusion, conflict resolution and explanations."@en .


############################
# Agents + Roles + Capabilities
############################

hi-s1:AIAssistant a hi:ArtificialAgent ;
    hi:hasAgentConcept hint:ArtificialAgent ;
    hi:hasRoleConcept hint:AIAssistantRole ;
    hi:hasCapability hi-s1:CapabilitySensing ,
                     hi-s1:CapabilityReasoning ,
                     hi-s1:CapabilityExplanation ;
    hi:performsExecution hi-s1:ExecutionConflictResolution ,
                         hi-s1:ExecutionExplanation .

hi-s1:HumanUser a hi:HumanAgent ;
    hi:hasAgentConcept hint:HumanAgent ;
    hi:hasRoleConcept hint:HumanUserRole ;
    hi:hasCapability hi-s1:CapabilitySelfReporting ;
    hi:performsExecution hi-s1:ExecutionDataCollection .


############################
# Capabilities
############################

hi-s1:CapabilitySensing a hi:Capability ;
    hi:hasCapabilityConcept hint:SensingCapability ;
    hi:allowsTask hi-s1:TaskDataCollection .

hi-s1:CapabilitySelfReporting a hi:Capability ;
    hi:hasCapabilityConcept hint:SelfReportingCapability ;
    hi:allowsTask hi-s1:TaskDataCollection .

hi-s1:CapabilityReasoning a hi:Capability ;
    hi:hasCapabilityConcept hint:ReasoningCapability ;
    hi:allowsTask hi-s1:TaskConflictResolution .

hi-s1:CapabilityExplanation a hi:Capability ;
    hi:hasCapabilityConcept hint:ExplainingCapability ;
    hi:allowsTask hi-s1:TaskExplanation .


############################
# Tasks
############################

hi-s1:TaskDataCollection a hi:Task ;
    hi:hasTaskConcept hint:DataCollectionTask ;
    hi:requiresCapability hi-s1:CapabilitySensing , hi-s1:CapabilitySelfReporting ;
    rdfs:comment "Collect user self-reports and sensor-based activity data."@en .

hi-s1:TaskConflictResolution a hi:Task ;
    hi:hasTaskConcept hint:ConflictResolutionTask ;
    hi:requiresCapability hi-s1:CapabilityReasoning ;
    rdfs:comment "Detect and resolve inconsistencies between reported and sensed data."@en .

hi-s1:TaskExplanation a hi:Task ;
    hi:hasTaskConcept hint:ExplanationTask ;
    hi:requiresCapability hi-s1:CapabilityExplanation ;
    rdfs:comment "Provide explanations to maintain trust and engagement."@en .


############################
# Task Executions 
############################

hi-s1:ExecutionDataCollection a hi:TaskExecution ;
    hi:hasExecutionConcept hint:DataCollectionTaskExecution ;
    hi:hasMethodConcept hint:DataCollectionMethod ;
    hi:realizesTask hi-s1:TaskDataCollection ;
    hi:performedBy hi-s1:HumanUser ;
    hi:towardsGoal hi-s1:GoalUserWellbeing ;
    hi:evaluatedBy hi-s1:EvaluationDataCollectionQuality .

hi-s1:ExecutionConflictResolution a hi:TaskExecution ;
    hi:hasExecutionConcept hint:ConflictResolutionTaskExecution ;
    hi:hasMethodConcept hint:ArgumentationReasoningMethod ;
    hi:realizesTask hi-s1:TaskConflictResolution ;
    hi:performedBy hi-s1:AIAssistant ;
    hi:towardsGoal hi-s1:GoalUserWellbeing ;
    hi:evaluatedBy hi-s1:EvaluationConflictAccuracy .

hi-s1:ExecutionExplanation a hi:TaskExecution ;
    hi:hasExecutionConcept hint:ExplanationTaskExecution ;
    hi:hasMethodConcept hint:PostHocExplanationMethod ;
    hi:realizesTask hi-s1:TaskExplanation ;
    hi:performedBy hi-s1:AIAssistant ;
    hi:towardsGoal hi-s1:GoalUserWellbeing ;
    hi:evaluatedBy hi-s1:EvaluationTransparencyImpact ;
    hi:hasInteractionEpisode hi-s1:InteractionExplainDecision .


############################
# Interactions
############################

hi-s1:InteractionExplainDecision a hi:Interaction ;
    hi:hasAgentInvolved hi-s1:AIAssistant , hi-s1:HumanUser ;
    hi:hasInteractionIntentConcept hint:ExplanationIntent ;
    hi:hasInteractionModalityConcept hint:VerbalDialogueModality .


############################
# Evaluations 
############################


hi-s1:EvaluationDataCollectionQuality a hi:Evaluation ;
    hi:hasEvaluationConcept hint:DataCollectionQualityEvaluation ;
    hi:hasMetricConcept hint:RecommendationRelevanceMetric ;
    hi:hasExperiment hi-s1:ExperimentDataCollectionQuality ;
    rdfs:comment "Evaluates the completeness, reliability, and relevance of collected data."@en .


hi-s1:EvaluationConflictAccuracy a hi:Evaluation ;
    hi:hasEvaluationConcept hint:ConflictResolutionAccuracyEvaluation ;
    hi:hasMetricConcept hint:FairOutcomeMetric ;
    hi:hasExperiment hi-s1:ExperimentConflictAccuracy ;
    rdfs:comment "Evaluates how accurately inconsistencies are identified and resolved."@en .


hi-s1:EvaluationTransparencyImpact a hi:Evaluation ;
    hi:hasEvaluationConcept hint:TransparencyEvaluation ;
    hi:hasMetricConcept hint:TrustPreservationMetric ;
    hi:hasExperiment hi-s1:ExperimentTransparencyTrust ;
    rdfs:comment "Evaluates how explanation clarity influences user trust under inconsistent data."@en .


############################
# Experiments + Hypotheses + Metrics
############################

hi-s1:ExperimentDataCollectionQuality a hi:Experiment ;
    hi:hasExperimentConcept hint:TransparencyTrustExperiment ;
    hi:hasMetricTested hint:RecommendationRelevanceMetric ;
    hi:hasNullHypothesis "Data collection quality does not improve user outcomes." ;
    hi:hasAlternativeHypothesis "Better data collection improves personalization and outcomes." .

hi-s1:ExperimentConflictAccuracy a hi:Experiment ;
    hi:hasExperimentConcept hint:DeliberationSupportExperiment ;
    hi:hasMetricTested hint:FairOutcomeMetric ;
    hi:hasNullHypothesis "Conflict resolution does not improve data consistency." ;
    hi:hasAlternativeHypothesis "Argumentation-based conflict resolution increases consistency." .

hi-s1:ExperimentTransparencyTrust a hi:Experiment ;
    hi:hasExperimentConcept hint:TransparencyTrustExperiment ;
    hi:hasMetricTested hint:TrustPreservationMetric ;
    hi:hasNullHypothesis "Transparency does not affect trust." ;
    hi:hasAlternativeHypothesis "Transparency improves user trust." .



#################################################################
# USE CASE 2
#################################################################

hi-s2:UseCaseCollaborativeManipulation a hi:UseCase ;
    hi:hasUseCaseConcept hint:CollaborativeManipulationUseCase ;
    hi:hasDomainConcept hint:IndustrialAndServiceRoboticsDomain ;
    hi:hasHITeam hi-s2:HITeamCollabManipulation .

#################################################################
# HI Team, Goal, Context, Ethical Constraints
#################################################################

hi-s2:HITeamCollabManipulation a hi:HITeam ;
    hi:hasGoal hi-s2:GoalSafeToolRetrieval ;
    hi:hasMember hi-s2:RobotManipulator , hi-s2:HumanTechnician ;
    hi:operatesInContext hi-s2:ContextWorkspaceManipulation ;

hi-s2:GoalSafeToolRetrieval a hi:Goal ;
    hi:hasGoalConcept hint:SafeRetrievalGoal ;
    hi:hasTask hi-s2:TaskUnlockCabinet ,
               hi-s2:TaskOpenCabinet ,
               hi-s2:TaskRetrieveTool .

hi-s2:ContextWorkspaceManipulation a hi:Context ;
    hi:hasContextConcept hint:WorkspaceManipulationContext ;
    hi:hasPhenomenonConcept hint:RiskExposurePhenomenon ,
                            hint:SkillsComplementarityPhenomenon .
	hi:hasConstraintConcept hint:SafetyConstraint ,
						    hint:TransparencyConstraint .						

#################################################################
# Agents (with role concepts and capabilities)
#################################################################

hi-s2:RobotManipulator a hi:ArtificialAgent ;
    hi:hasAgentConcept hint:ArtificialAgent ;
    hi:hasRoleConcept hint:RiskOperatorRole ;
    hi:hasCapability hi-s2:CapabilityHeavyLifting ,
                     hi-s2:CapabilityConfinedReach ,
                     hi-s2:CapabilityRLPolicyLearning ,
                     hi-s2:CapabilityExplanation ,
                     hi-s2:CapabilityKGQuery ;
    hi:performsExecution hi-s2:ExecConfinedReach ,
                         hi-s2:ExecOpenCabinet ,
                         hi-s2:ExecHeavyLift ,
                         hi-s2:ExecRLExplain .

hi-s2:HumanTechnician a hi:HumanAgent ;
    hi:hasAgentConcept hint:HumanAgent ;
    hi:hasRoleConcept hint:FineManipulatorRole ;
    hi:hasCapability hi-s2:CapabilityFineDexterity ,
                     hi-s2:CapabilitySituationalAwareness ,
                     hi-s2:CapabilitySelfReporting ;
    hi:performsExecution hi-s2:ExecUnlockCabinet .

#################################################################
# Capabilities
#################################################################

hi-s2:CapabilityHeavyLifting a hi:Capability ;
    hi:hasCapabilityConcept hint:HeavyLiftingCapability ;
    hi:allowsTask hi-s2:TaskHeavyLift ,
                  hi-s2:TaskRetrieveTool .

hi-s2:CapabilityConfinedReach a hi:Capability ;
    hi:hasCapabilityConcept hint:ConfinedReachingCapability ;
    hi:allowsTask hi-s2:TaskOpenCabinet ,
                  hi-s2:TaskHeavyLift .

hi-s2:CapabilityRLPolicyLearning a hi:Capability ;
    hi:hasCapabilityConcept hint:ReinforcementLearningCapability ;
    hi:allowsTask hi-s2:TaskPolicyLearning .

hi-s2:CapabilityExplanation a hi:Capability ;
    hi:hasCapabilityConcept hint:ExplainingCapability ;
    hi:allowsTask hi-s2:TaskExplanation .

hi-s2:CapabilityKGQuery a hi:Capability ;
    hi:hasCapabilityConcept hint:KnowledgeGraphQueryingCapability ;
    hi:allowsTask hi-s2:TaskKnowledgeQuery .

hi-s2:CapabilityFineDexterity a hi:Capability ;
    hi:hasCapabilityConcept hint:FineDexterityCapability ;
    hi:allowsTask hi-s2:TaskFineInsertion ,
                  hi-s2:TaskUnlockCabinet .

hi-s2:CapabilitySituationalAwareness a hi:Capability ;
    hi:hasCapabilityConcept hint:SituationalAwarenessCapability ;
    hi:allowsTask hi-s2:TaskSituationalAssessment .

hi-s2:CapabilitySelfReporting a hi:Capability ;
    hi:hasCapabilityConcept hint:SelfReportingCapability ;
    hi:allowsTask hi-s2:TaskDataCollection .

#################################################################
# Tasks
#################################################################

hi-s2:TaskUnlockCabinet a hi:Task ;
    hi:hasTaskConcept hint:UnlockCabinetTask ;
    hi:requiresCapability hi-s2:CapabilityFineDexterity .

hi-s2:TaskOpenCabinet a hi:Task ;
    hi:hasTaskConcept hint:OpenCabinetTask ;
    hi:requiresCapability hi-s2:CapabilityConfinedReach .

hi-s2:TaskRetrieveTool a hi:Task ;
    hi:hasTaskConcept hint:RetrieveToolTask ;
    hi:requiresCapability hi-s2:CapabilityHeavyLifting .

hi-s2:TaskFineInsertion a hi:Task ;
    hi:hasTaskConcept hint:FineInsertionTask ;
    hi:requiresCapability hi-s2:CapabilityFineDexterity .

hi-s2:TaskHeavyLift a hi:Task ;
    hi:hasTaskConcept hint:HeavyLiftingTask ;
    hi:requiresCapability hi-s2:CapabilityHeavyLifting ,
                          hi-s2:CapabilityConfinedReach .

hi-s2:TaskSituationalAssessment a hi:Task ;
    hi:hasTaskConcept hint:SituationalAssessmentTask ;
    hi:requiresCapability hi-s2:CapabilitySituationalAwareness .

hi-s2:TaskDataCollection a hi:Task ;
    hi:hasTaskConcept hint:DataCollectionTask ;
    hi:requiresCapability hi-s2:CapabilitySelfReporting .

hi-s2:TaskPolicyLearning a hi:Task ;
    hi:hasTaskConcept hint:PolicyLearningTask ;
    hi:requiresCapability hi-s2:CapabilityRLPolicyLearning .

hi-s2:TaskKnowledgeQuery a hi:Task ;
    hi:hasTaskConcept hint:KnowledgeGraphQueryTask ;
    hi:requiresCapability hi-s2:CapabilityKGQuery .

hi-s2:TaskExplanation a hi:Task ;
    hi:hasTaskConcept hint:ExplanationTask ;
    hi:requiresCapability hi-s2:CapabilityExplanation .


#################################################################
# Task Executions (each with its own evaluation)
#################################################################

hi-s2:ExecUnlockCabinet a hi:TaskExecution ;
    hi:hasExecutionConcept hint:UnlockCabinetTaskExecution ;
    hi:realizesTask hi-s2:TaskUnlockCabinet ;
    hi:performedBy hi-s2:HumanTechnician ;
    hi:towardsGoal hi-s2:GoalSafeToolRetrieval ;
    hi:evaluatedBy hi-s2:EvalFineManipulation .

hi-s2:ExecOpenCabinet a hi:TaskExecution ;
    hi:hasExecutionConcept hint:OpenCabinetTaskExecution ;
    hi:realizesTask hi-s2:TaskOpenCabinet ;
    hi:performedBy hi-s2:RobotManipulator ;
    hi:hasMethodConcept hint:KnowledgeGraphQueryMethod ;
    hi:towardsGoal hi-s2:GoalSafeToolRetrieval ;
    hi:evaluatedBy hi-s2:EvalKGQueryAccuracy .

hi-s2:ExecConfinedReach a hi:TaskExecution ;
    hi:hasExecutionConcept hint:ConfinedReachTaskExecution ;
    hi:realizesTask hi-s2:TaskHeavyLift ;
    hi:performedBy hi-s2:RobotManipulator ;
    hi:hasMethodConcept hint:ReinforcementLearningMethod ;
    hi:towardsGoal hi-s2:GoalSafeToolRetrieval ;
    hi:evaluatedBy hi-s2:EvalReachSafety .

hi-s2:ExecHeavyLift a hi:TaskExecution ;
    hi:hasExecutionConcept hint:HeavyLiftingTaskExecution ;
    hi:realizesTask hi-s2:TaskHeavyLift ;
    hi:performedBy hi-s2:RobotManipulator ;
    hi:towardsGoal hi-s2:GoalSafeToolRetrieval ;
    hi:evaluatedBy hi-s2:EvalLoadSafety .

hi-s2:ExecRLExplain a hi:TaskExecution ;
    hi:hasExecutionConcept hint:ReinforcementLearningExplanationTaskExecution ;
    hi:realizesTask hi-s2:TaskExplanation ;
    hi:performedBy hi-s2:RobotManipulator ;
    hi:hasMethodConcept hint:PostHocExplanationMethod ;
    hi:towardsGoal hi-s2:GoalSafeToolRetrieval ;
    hi:evaluatedBy hi-s2:EvalExplanationTransparency .

#################################################################
# Evaluations 
#################################################################

hi-s2:EvalFineManipulation a hi:Evaluation ;
    hi:hasEvaluationConcept hint:FineManipulationEvaluation ;
    hi:hasMetricConcept hint:TaskSuccessRateMetric ;
    hi:hasExperiment hi-s2:ExperimentFineManipulation .

hi-s2:EvalKGQueryAccuracy a hi:Evaluation ;
    hi:hasEvaluationConcept hint:KGQueryAccuracyEvaluation ;
    hi:hasMetricConcept hint:TaskSuccessRateMetric ;
    hi:hasExperiment hi-s2:ExperimentKGSupport .

hi-s2:EvalReachSafety a hi:Evaluation ;
    hi:hasEvaluationConcept hint:AIInterventionImpactEvaluation ;
    hi:hasMetricConcept hint:SafetyComplianceMetric ;
    hi:hasExperiment hi-s2:ExperimentReachSafety .

hi-s2:EvalLoadSafety a hi:Evaluation ;
    hi:hasEvaluationConcept hint:AIInterventionImpactEvaluation ;
    hi:hasMetricConcept hint:SafetyComplianceMetric ;
    hi:hasExperiment hi-s2:ExperimentLoadSafety .

hi-s2:EvalExplanationTransparency a hi:Evaluation ;
    hi:hasEvaluationConcept hint:TransparencyEvaluation ;
    hi:hasMetricConcept hint:ExplanationClarityMetric ;
    hi:hasExperiment hi-s2:ExperimentExplanationTransparency .

#################################################################
# Experiments 
#################################################################

hi-s2:ExperimentFineManipulation a hi:Experiment ;
    hi:hasExperimentConcept hint:FineManipulationExperiment ;
    hi:hasMetricTested hint:TaskSuccessRateMetric ;
    hi:hasNullHypothesis "Fine manipulation precision and success rate do not improve across trials or conditions." ;
    hi:hasAlternativeHypothesis "Fine manipulation precision and success rate improve, especially when roles and capabilities are well allocated between human and robot." .

hi-s2:ExperimentKGSupport a hi:Experiment ;
    hi:hasExperimentConcept hint:ReinforcementLearningWithKnowledgeGraphsExperiment ;
    hi:hasMetricTested hint:TaskSuccessRateMetric ;
    hi:hasNullHypothesis "Knowledge graph support does not significantly increase the success rate of cabinet opening and tool retrieval." ;
    hi:hasAlternativeHypothesis "Knowledge graph support increases the success rate and robustness of cabinet opening and tool retrieval sequences." .

hi-s2:ExperimentReachSafety a hi:Experiment ;
    hi:hasExperimentConcept hint:SafetyComplianceExperiment ;
    hi:hasMetricTested hint:SafetyComplianceMetric ;
    hi:hasNullHypothesis "Using RL-based policies in confined reach does not change safety compliance compared to baseline control." ;
    hi:hasAlternativeHypothesis "Using RL-based policies in confined reach improves safety compliance and reduces risk exposure for the human." .

hi-s2:ExperimentLoadSafety a hi:Experiment ;
    hi:hasExperimentConcept hint:SafetyComplianceExperiment ;
    hi:hasMetricTested hint:SafetyComplianceMetric ;
    hi:hasNullHypothesis "Delegating heavy lifting to the robot does not significantly reduce safety incidents." ;
    hi:hasAlternativeHypothesis "Delegating heavy lifting to the robot reduces safety incidents and improves overall safety compliance." .

hi-s2:ExperimentExplanationTransparency a hi:Experiment ;
    hi:hasExperimentConcept hint:TransparencyTrustExperiment ;
    hi:hasMetricTested hint:ExplanationClarityMetric ;
    hi:hasNullHypothesis "Providing explanations about planned risky actions does not affect perceived clarity or operator trust." ;
    hi:hasAlternativeHypothesis "Providing explanations about planned risky actions improves perceived clarity and maintains or increases operator trust." .

