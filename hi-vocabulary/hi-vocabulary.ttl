hint:HIScheme a skos:ConceptScheme ;
    dct:title "Hybrid INtelligence Thesaurus (HINT)"@en ;
    dct:description "A controlled thesaurus of terms to describe Hybrid Intelligence (HI)."@en ;
    dc:creator [
        a foaf:Person ;
        foaf:name "Francesca Urgese" ;
        schema:identifier [
            a schema:PropertyValue ;
            schema:propertyID "ORCID" ;
            schema:value "0009-0009-0516-1800";
            foaf:member _:org  
        ]], [
        a foaf:Person ;
        foaf:name "Margherita Martorana" ;
        schema:identifier [
            a schema:PropertyValue ;
            schema:propertyID "ORCID" ;
            schema:value "0000-0001-8004-0464" ;
            foaf:member _:org  
        ]] , [
        a foaf:Person ;
        foaf:name "Ilaria Tiddi" ;
        schema:identifier [
            a schema:PropertyValue ;
            schema:propertyID "ORCID" ;
            schema:value "0000-0001-7116-9338" ;
            foaf:member _:org  
        ]] ;
    dct:license <https://creativecommons.org/licenses/by/4.0/> ;
    dcterms:created "2025-11-15"^^xsd:date ;
    vann:preferredNamespacePrefix "hint";
    vann:preferredNamespaceUri "https://w3id.org/hi-thesaurus#" ;
    skos:prefLabel "Hybrid INtelligence Thesaurus"@en ;
    skos:hasTopConcept ct:Sensing ,
                       ct:Reasoning ,
                       ct:Explaining .

_:org a schema:Organization ;
    schema:name "Knowledge in AI (KAI) Group - Vrije Universiteit Amsterdam" .



